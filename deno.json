{
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check .",
    "clean": "rm -rf node_modules && rm -rf dist",
    "dev": "vite",
    "make": "vite build --mode client && vite build",
    "prod": "vite preview"
  },
  "exclude": ["dist"],
  "nodeModulesDir": "auto",
  "imports": {
    "hono": "npm:hono",
    "honox": "npm:honox",
    "vite": "npm:vite",
    "@hono/vite-ssg": "npm:@hono/vite-ssg",
    "@mdx-js/rollup": "npm:@mdx-js/rollup",
    "remark-frontmatter": "npm:remark-frontmatter",
    "@hono/vite-build/deno": "npm:@hono/vite-build/deno",
    "remark-mdx-frontmatter": "npm:remark-mdx-frontmatter"
  },
  "lock": false
}
